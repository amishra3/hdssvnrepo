<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    helpPath="en/cq/current/wcm/default_components.html#List"
    manageTabs="function(tab,noSwitch){var tabs=['props','homepagehero','homepagesecondary','homepagesecondarytype2','productsolutionhero','storagehero','productdetailvideohero','newsandinsightsshorthero'];var index=tabs.indexOf(tab);if(index==-1) return;for(var i=1;i&lt;tabs.length;i++){if(index==i){this.unhideTabStripItem(i);}else{this.hideTabStripItem(i);}}this.doLayout();if(!noSwitch)this.activate(index);}"
    title="List"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <list
            jcr:primaryType="cq:Widget"
            title="List"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <listFrom
                    jcr:primaryType="cq:Widget"
                    defaultValue=""
                    fieldLabel="Select Banner Type"
                    name="./listFrom"
                    type="select"
                    xtype="selection">
                    <listeners
                        jcr:primaryType="nt:unstructured"
                        loadcontent="function(){this.findParentByType('tabpanel').manageTabs(this.getValue(),true);}"
                        selectionchanged="function(box,value){box.findParentByType('tabpanel').manageTabs(value);}"/>
                    <options jcr:primaryType="cq:WidgetCollection">
                        <homepagehero
                            jcr:primaryType="nt:unstructured"
                            text="Homepage Hero Banner"
                            value="homepagehero"/>
                        <homepagesecondary
                            jcr:primaryType="nt:unstructured"
                            text="Homepage Seondary Banner"
                            value="homepagesecondary"/>
                        <homepagesecondarytype2
                            jcr:primaryType="nt:unstructured"
                            text="Homepage Secondary Banner Type 2"
                            value="homepagesecondarytype2"/>
                        <productsolutionhero
                            jcr:primaryType="nt:unstructured"
                            text="Product &amp; Solution Hero Banner"
                            value="productsolutionhero"/>
                        <storagehero
                            jcr:primaryType="nt:unstructured"
                            text="Storage Hero Banner"
                            value="storagehero"/>
                        <productdetailvideohero
                            jcr:primaryType="nt:unstructured"
                            text="Product Detail Video Hero banner"
                            value="productdetailvideohero"/>
                        <newsandinsightsshorthero
                            jcr:primaryType="nt:unstructured"
                            text="News and Insights Short Hero banner"
                            value="newsandinsightsshorthero"/>
                    </options>
                </listFrom>
            </items>
        </list>
        <homepagehero
            jcr:primaryType="cq:Widget"
            title="Homepage Hero Banner"
            xtype="panel">
            <listeners
                jcr:primaryType="nt:unstructured"
                render="function() { this.findParentByType('tabpanel').hideTabStripItem(1); }"/>
            <items jcr:primaryType="cq:WidgetCollection">
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                    allowBlank="{Boolean}false"
                    fieldLabel="Banner Title"
                    name="./jcr:title"
                    required="{Boolean}true"/>
                <altText
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                    allowBlank="{Boolean}false"
                    fieldLabel="Image ALT text"
                    name="./jcr:altText"
                    required="{Boolean}true"/>
                <subText
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                    allowBlank="{Boolean}false"
                    fieldLabel="Banner Sub text"
                    name="./jcr:subText"
                    required="{Boolean}true"/>
                <viewalllink
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                    fieldLabel="View ALL link"
                    name="./jcr:viewalllink"/>
                <viewalltext
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                    fieldLabel="View ALL link text"
                    name="./jcr:viewalltext"/>
                <imagePath
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    allowUpload="false"
                    cropParameter="./imagePath/imageCrop2"
                    ddGroups="[media]"
                    fieldLabel="Banner Image"
                    fileNameParameter="./imagePath/fileName2"
                    fileReferenceParameter="./jcr:imagePath"
                    height="{Long}200"
                    name="./imagePath/file2"
                    rotateParameter="./imagePath/imageRotate2"
                    sizeLimit="100"
                    title="Image"
                    width="{Long}200"
                    xtype="html5smartimage"/>
                <tab1
                    jcr:primaryType="cq:Panel"
                    title="IndustryTab ">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <map
                            jcr:primaryType="cq:Widget"
                            hideLabel="false"
                            name="./map"
                            title="Attributes"
                            xtype="multifield">
                            <fieldConfig
                                jcr:primaryType="cq:Widget"
                                border="true"
                                hideLabel="true"
                                layout="form"
                                padding="10px"
                                width="1000"
                                xtype="mymultipanel">
                                <items jcr:primaryType="cq:WidgetCollection">
                                    <displaylabel
                                        jcr:primaryType="cq:Widget"
                                        dName="displaylabel"
                                        fieldLabel="Display Label"
                                        name="./jcr:displaylabel"
                                        width="100"
                                        xtype="textfield"/>
                                    <iconimagepath
                                        jcr:primaryType="cq:Widget"
                                        dName="iconimagepath"
                                        fieldLabel="Icon Image Path"
                                        name="./jcr:iconimagepath"
                                        width="300"
                                        xtype="pathfield"/>
                                    <targeturl
                                        jcr:primaryType="cq:Widget"
                                        dName="targeturl"
                                        fieldLabel="Target URL"
                                        name="./jcr:targeturl"
                                        width="300"
                                        xtype="pathfield"/>
                                    <iconimage
                                        jcr:primaryType="cq:Widget"
                                        dName="iconimage"
                                        fieldLabel="Icon Image"
                                        name="./jcr:iconimage"
                                        width="300"
                                        xtype="pathfield"/>
                                </items>
                            </fieldConfig>
                        </map>
                    </items>
                </tab1>
            </items>
        </homepagehero>
