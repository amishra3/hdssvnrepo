<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contactbannercontent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Content"
                        name="./contactbannercontent"
                        xtype="textfield">
                        <rtePlugins
                            jcr:primaryType="nt:unstructured"
                            path="/apps/hdscorp/components/content/richtext/tab_editor/items/editor/rtePlugins.infinity.json"
                            xtype="cqinclude"/>
                    </contactbannercontent>
                    <contactusbannerphonelinktext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Find A Seller Link Label"
                        name="./phonelinktext"
                        xtype="textfield"/>
                    <contactusbannerphonelinkurl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Find A Seller Link Target URL"
                        name="./phonelinkurl"
                        xtype="pathfield"/>
                    <contactusphonelinkurltargettype
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Open in New Window ?"
                        name="./phonetargettype"
                        type="checkbox"
                        xtype="selection"/>
                    <contactusbannerbuttonlabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Get Information Button Label"
                        name="./contactusbannerbuttonlabel"
                        xtype="textfield"/>
                    <contactusbannerbuttonurl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Get Information Button Target URL"
                        name="./contactusbannerbuttonurl"
                        xtype="pathfield"/>
                    <thirdpartybutton
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Show third party icon?"
                        key="thirdpartybutton"
                        name="./thirdpartybutton"
                        type="checkbox"
                        xtype="selection"/>
                    <contactusbannerurltargettype
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Open in New Window ?"
                        name="./contactusbannerurltargettype"
                        type="checkbox"
                        xtype="selection"/>
                    <contactusbannerlinktext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Find A Seller Link Label"
                        name="./contactusbannerlinktext"
                        xtype="textfield"/>
                    <contactusbannerlinkurl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Find A Seller Link Target URL"
                        name="./contactusbannerlinkurl"
                        xtype="pathfield"/>
                    <contactuslinkurltargettype
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Open in New Window ?"
                        name="./contactuslinkurltargettype"
                        type="checkbox"
                        xtype="selection"/>
                    <thirdparty
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Show third party icon?"
                        key="thirdparty"
                        name="./thirdparty"
                        type="checkbox"
                        xtype="selection"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
