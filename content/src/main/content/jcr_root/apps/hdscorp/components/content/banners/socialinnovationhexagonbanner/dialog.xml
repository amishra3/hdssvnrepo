<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <hexagonbannertitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Title"
                        name="./hexagonbannertitle"
                        xtype="textfield"/>
                    <hexagonbannerdescription
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Description"
                        name="./hexagonbannerdescription"
                        xtype="textfield"/>
                    <hexagonbannertitleimagePath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        allowUpload="false"
                        cropParameter="./hexagonbannertitleimageCrop2"
                        ddGroups="[media]"
                        fieldLabel="Banner Image"
                        fileNameParameter="./aboutusheximagefileName2"
                        fileReferenceParameter="./hexagonbannertitleimagePath"
                        height="{Long}200"
                        name="./hexagonbannertitleimagePath"
                        rotateParameter="./hexagonbannertitleimagePathRotate2"
                        sizeLimit="100"
                        title="Image"
                        width="{Long}200"
                        xtype="html5smartimage"/>
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./aboutushexmultilinks"
                        fieldLabel="Tabs"
                        name="./aboutushexmultilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <placardTitle
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Title"
                                name="./placardTitle"
                                xtype="textfield"/>
                            <placardcontent
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Content"
                                name="./placardcontent"
                                xtype="textarea"/>
                            <placardIconPath
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Icon Path"
                                name="./placardIconPath"
                                xtype="pathfield"/>
                            <placardIconalt
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Icon Alt"
                                name="./placardIconalt"
                                xtype="textfield"/>
                            <placardurllabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard URL Label"
                                name="./placardurllabel"
                                xtype="textfield"/>
                            <placardtargeturl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Target URL"
                                name="./placardtargeturl"
                                xtype="pathfield"/>
                            <openinnewwindow
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window ?"
                                name="./openinnewwindow"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </openinnewwindow>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
