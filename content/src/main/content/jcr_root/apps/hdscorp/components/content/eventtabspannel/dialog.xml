<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    title="Event Tabs Panel"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tabs
                jcr:primaryType="cq:Panel"
                title="Tab">
                <items
                    jcr:primaryType="cq:WidgetCollection"
                    xtype="multifieldpanel">
                    <etpmultifield
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Add Tab"
                        name="./etpmultifield"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <etptabname
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="{Boolean}false"
                                    fieldLabel="Tab Name"
                                    key="etptabname"
                                    name="./etptabname"
                                    xtype="textfield"/>
                                <etptabid
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="{Boolean}false"
                                    fieldLabel="Tab Id"
                                    key="etptabid"
                                    name="./etptabid"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </etpmultifield>
                </items>
            </tabs>
        </items>
    </items>
</jcr:root>
