<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <_x0034_04backgroundimage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Background Image"
                        name="./nfbackgroundimage"
                        xtype="pathfield"/>
                    <_x0034_04title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./nftitle"
                        xtype="textfield"/>
                    <_x0034_04subtitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Content"
                        name="./nfsubtitle"
                        xtype="richtext"/>
                    <_x0034_04multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./404multilinks"
                        fieldLabel="Links"
                        name="./404multilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <_x0034_04linklabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Link Label"
                                name="./nflinklabel"
                                xtype="textfield"/>
                            <_x0034_04link
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Link"
                                name="./nflink"
                                xtype="pathfield"/>
                            <_x0034_04openinnewwindow
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window"
                                name="./nfopeninnewwindow"
                                type="select"
                                width="300"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <opt1
                                        jcr:primaryType="nt:unstructured"
                                        text="YES"
                                        value="1"/>
                                    <opt2
                                        jcr:primaryType="nt:unstructured"
                                        text="NO"
                                        value="2"/>
                                </options>
                            </_x0034_04openinnewwindow>
                        </fieldConfig>
                    </_x0034_04multilinks>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
