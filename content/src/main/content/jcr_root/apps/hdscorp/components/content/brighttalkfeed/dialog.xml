<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Brighttalk Webcast Component"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <playerpagepath
            jcr:primaryType="cq:Widget"
            fieldDescription="Please select the path for the page which renders the BrightTalk player."
            fieldLabel="BrightTalk player page path"
            name="./playerpagepath"
            xtype="ddpathfield"/>
        <ctatargeturlopeninnew
            jcr:primaryType="cq:Widget"
            fieldLabel="Open in Modal ?"
            name="./playeropeninmodal"
            type="checkbox"
            xtype="selection"/>
    </items>
</jcr:root>
