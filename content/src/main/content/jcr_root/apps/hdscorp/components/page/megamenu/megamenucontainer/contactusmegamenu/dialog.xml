<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <mgmcontactusnavtitle
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Navegation Title"
                        name="./mgmcontactusnavtitle"
                        xtype="textfield"/>
                    <mgmcontactusnavpath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Navigation Path"
                        name="./mgmcontactusnavpath"
                        xtype="pathfield"/>
                    <mgmcontactustitle
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Title"
                        name="./mgmcontactustitle"
                        xtype="textfield"/>
                    <mgmcontactusiconpath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Icon Path"
                        name="./mgmcontactusiconpath"
                        xtype="pathfield"/>
                    <mgmcontactusbackgroundimagepath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Background Image Path"
                        name="./mgmcontactusbackgroundimagepath"
                        xtype="pathfield"/>
                    <mgconmobilebackgroundimagepath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Mobile BackGround Image Path"
                        name="./mgconmobilebackgroundimagepath"
                        xtype="pathfield"/>
                    <subnav
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./subnav"
                        fieldLabel="&#xa;Sub Navigation Path"
                        name="./subnav"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <mgmcontactussubnavlabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Sub Navgation Label"
                                name="./mgmcontactussubnavlabel"
                                xtype="textfield"/>
                            <mgmcontactussubnavlink
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Sub Navigation Path"
                                name="./mgmcontactussubnavlink"
                                xtype="pathfield"/>
                            <mgmcontactusalttext
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Link  Alt Text"
                                name="./mgmcontactusalttext"
                                xtype="textfield"/>
                            <mgmcontactusopeninnewwindow
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window?"
                                key="mgmcontactusopeninnewwindow"
                                name="./mgmcontactusopeninnewwindow"
                                type="select"
                                width="300"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <opt1
                                        jcr:primaryType="nt:unstructured"
                                        text="YES"
                                        value="1"/>
                                    <opt2
                                        jcr:primaryType="nt:unstructured"
                                        text="NO"
                                        value="2"/>
                                </options>
                            </mgmcontactusopeninnewwindow>
                        </fieldConfig>
                    </subnav>
                    <mgmcontactusmultidescription
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./mgmcontactusmultidescription"
                        fieldLabel="&#xa;Description"
                        name="./mgmcontactusmultidescription"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <mgmcontactusheadline
                                jcr:primaryType="cq:Widget"
                                fieldLabel="HeadLine"
                                name="./mgmcontactusheadline"
                                xtype="textarea"/>
                            <mgmcontactusdescription
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Description"
                                name="./mgmcontactusdescription"
                                xtype="richtext"/>
                            <mgmcontactusviewfeaturedproductslabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="View Featured Products Label"
                                name="./mgmcontactusviewfeaturedproductslabel"
                                xtype="textfield"/>
                            <mgmcontactusnumber
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Contact Us Number"
                                name="./mgmcontactusnumber"
                                xtype="textfield"/>
                            <mgmcontactusviewfeaturedproductslink
                                jcr:primaryType="cq:Widget"
                                fieldLabel="View Featured Products Path"
                                name="./mgmcontactusviewfeaturedproductslink"
                                xtype="pathfield"/>
                            <overlay
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in marketo Form"
                                name="./overlay"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </overlay>
                            <mgmcontactusopeninnewwindow2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window?"
                                key="mgmcontactusopeninnewwindow2"
                                name="./mgmcontactusopeninnewwindow2"
                                type="select"
                                width="300"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <opt1
                                        jcr:primaryType="nt:unstructured"
                                        text="YES"
                                        value="1"/>
                                    <opt2
                                        jcr:primaryType="nt:unstructured"
                                        text="NO"
                                        value="2"/>
                                </options>
                            </mgmcontactusopeninnewwindow2>
                        </fieldConfig>
                    </mgmcontactusmultidescription>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
