<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    title="About HDS Vertical Explorer"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <abouthds
                jcr:primaryType="cq:Panel"
                title="tab">
                <items jcr:primaryType="cq:WidgetCollection">
                    <ahebgimagepath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Background Image"
                        name="./ahebgimagepath"
                        xtype="pathfield"/>
                    <aheiconbimagepath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Item Image Path"
                        name="./aheiconbimagepath"
                        xtype="pathfield"/>
                    <aheiconbdeviceimagepath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Item Device Image Path"
                        name="./deviceimagepath"
                        xtype="pathfield"/>
                    <aheiconbimagelabel
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Item Label"
                        name="./aheiconbimagelabel"
                        xtype="textfield"/>
                    <aheiconbimagedate
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Item Date"
                        name="./aheiconbimagedate"
                        xtype="textfield"/>
                    <aheiconbimagedesc
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Item Description"
                        name="./aheiconbimagedesc"
                        xtype="textfield"/>
                    <aheiconbimagedesclink
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Item Description Link"
                        name="./aheiconbimagedesclink"
                        xtype="textfield"/>
                    <openinnewwindow
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Open In New Window?"
                        name="./openinnewwindow"
                        type="checkbox"
                        xtype="selection"/>
                    <thirdparty
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Show third party icon?"
                        key="thirdparty"
                        name="./thirdparty"
                        type="checkbox"
                        xtype="selection"/>
                </items>
            </abouthds>
        </items>
    </items>
</jcr:root>
