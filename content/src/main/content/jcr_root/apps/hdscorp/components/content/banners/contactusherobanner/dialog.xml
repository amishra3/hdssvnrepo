<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contactustitlecontent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Title"
                        name="./contactustitlecontent"
                        xtype="textfield"/>
                    <contactusimagePath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        allowUpload="false"
                        cropParameter="./contactusimageCrop2"
                        ddGroups="[media]"
                        fieldLabel="Banner Image"
                        fileNameParameter="./contactusimagefileName2"
                        fileReferenceParameter="./contactusimagePath"
                        height="{Long}200"
                        name="./contactusimagePath"
                        rotateParameter="./contactusimagePathimageRotate2"
                        sizeLimit="100"
                        title="Image"
                        width="{Long}200"
                        xtype="html5smartimage"/>
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./contactusmultilinks"
                        fieldLabel="Tabs"
                        name="./contactusmultilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <placardTitle
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Title"
                                name="./placardTitle"
                                xtype="textfield"/>
                            <placardcontent
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Placard Content"
                                name="./placardcontent"
                                xtype="textarea">
                            </placardcontent>
                            <phonenumbertext1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Phone Number 1 (Optional)"
                                name="./phonenumbertext1"
                                xtype="textfield"/>
                            <phonenumbertext2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Phone Number 2 (Optional)"
                                name="./phonenumbertext2"
                                xtype="textfield"/>                                
                            <contacturllabel1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="URL 1 Label"
                                name="./contacturllabel1"
                                xtype="textfield"/>
                            <contacttargeturl1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Target URL 1 "
                                name="./contacttargeturl1"
                                xtype="pathfield"/>
                            <contacturllabel2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="URL 2 Label"
                                name="./contacturllabel2"
                                xtype="textfield"/>
                            <contacttargeturl2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Target URL 2 "
                                name="./contacttargeturl2"
                                xtype="pathfield"/>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
