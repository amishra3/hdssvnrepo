<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Contact US 3 Column Content"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Column 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contactuscolumntitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Column Title"
                        name="./contactuscolumntitle"
                        xtype="textfield"/>
                    <contactuscolumncontent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Content"
                        name="./contactuscolumncontent"
                        xtype="textarea"/>
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks1"
                        fieldLabel="Links"
                        name="./multilinks1"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <urllabeltab1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="URL Link Label"
                                name="./urllabeltab1"
                                xtype="textfield"/>
                            <urlltargettab1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Target URL"
                                name="./urlltargettab1"
                                xtype="ddpathfield"/>
                            <urltype
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Show third party icon?"
                                name="./urltype1"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </urltype>
                            <newwin
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window ?"
                                name="./newwin1"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </newwin>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Column 2 ">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contactuscolumntitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Column Title"
                        name="./contactuscolumntitle2"
                        xtype="textfield"/>
                    <contactuscolumncontent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Content"
                        name="./contactuscolumncontent2"
                        xtype="textarea"/>
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks2"
                        fieldLabel="Links"
                        name="./multilinks2"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <urllabeltab1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="URL Link Label"
                                name="./urllabeltab2"
                                xtype="textfield"/>
                            <urlltargettab1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Target URL"
                                name="./urlltargettab2"
                                xtype="ddpathfield"/>
                            <urltype
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Show third party icon?"
                                name="./urltype2"
                                type="select"
                                width="{Long}200"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </urltype>
                            <newwin
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window ?"
                                name="./newwin2"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </newwin>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab2>
            <tab3
                jcr:primaryType="cq:Panel"
                title="Column 3 ">
                <items jcr:primaryType="cq:WidgetCollection">
                    <contactuscolumntitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Column Title"
                        name="./contactuscolumntitle3"
                        xtype="textfield"/>
                    <contactuscolumncontent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Column Content"
                        name="./contactuscolumncontent3"
                        xtype="textarea"/>
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks3"
                        fieldLabel="Links"
                        name="./multilinks3"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <urllabeltab1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="URL Link Label"
                                name="./urllabeltab3"
                                xtype="textfield"/>
                            <urlltargettab1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Target URL"
                                name="./urlltargettab3"
                                xtype="ddpathfield"/>
                            <urltype
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Show third party icon?"
                                name="./urltype3"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </urltype>
                            <newwin
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window ?"
                                name="./newwin3"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </newwin>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab3>
        </items>
    </items>
</jcr:root>
