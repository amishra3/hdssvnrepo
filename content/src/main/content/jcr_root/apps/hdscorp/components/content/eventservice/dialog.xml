<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="300"
    title="dialog"
    width="500"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Event Service">
                <items jcr:primaryType="cq:WidgetCollection">
                    <esnoeventfoundmessage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="NO Event Found Message"
                        name="./esnoeventfoundmessage"
                        xtype="textfield"/>
                    <esdetailslabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Details Label"
                        name="./esdetailslabel"
                        xtype="textfield"/>
                    <eseventlookuppath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Event Lookup Path"
                        name="./eseventlookuppath"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
