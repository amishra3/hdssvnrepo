<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <tdalltraininglabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="All Training Label"
                        name="./tdalltraininglabel"
                        xtype="textfield"/>
                    <tdsearchbylocationlabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Serach By  Location Label"
                        name="./tdsearchbylocationlabel"
                        xtype="textfield"/>
                    <tdlocations
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks"
                        fieldLabel="Tabs"
                        name="./multilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <tdlocationid
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Location ID"
                                name="./tdlocationid"
                                xtype="textfield"/>
                            <tdlocationlabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Location Label"
                                name="./tdlocationlabel"
                                xtype="textfield"/>
                        </fieldConfig>
                    </tdlocations>
                    <tdpathlocationsearch
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Training Search Lookup Path"
                        name="./tdpathlocationsearch"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
