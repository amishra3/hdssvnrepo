<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    title="News Insights Blog Container"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <insightcontainer
                jcr:primaryType="cq:Panel"
                title="Tab">
                <items
                    jcr:primaryType="cq:WidgetCollection"
                    xtype="multifieldpanel">
                    <nivcmuultifield
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Add Section"
                        name="./nivcmuultifield"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <nilesectionlabel
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="{Boolean}false"
                                    fieldLabel="Section Label"
                                    key="nilesectionlabel"
                                    name="./nilesectionlabel"
                                    xtype="textfield"/>
                                <nilesectionlink
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="{Boolean}false"
                                    fieldLabel="Section Link"
                                    key="nilesectionlink"
                                    name="./nilesectionlink"
                                    xtype="pathfield"/>
                                <nileopeninnewwindow
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Open in New Window"
                                    key="nileopeninnewwindow"
                                    name="./nileopeninnewwindow"
                                    type="select"
                                    width="300"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <opt1
                                            jcr:primaryType="nt:unstructured"
                                            text="YES"
                                            value="1"/>
                                        <opt2
                                            jcr:primaryType="nt:unstructured"
                                            text="NO"
                                            value="2"/>
                                    </options>
                                </nileopeninnewwindow>
                            </items>
                        </fieldConfig>
                    </nivcmuultifield>
                </items>
            </insightcontainer>
        </items>
    </items>
</jcr:root>
