            text="Metadata Update"
            value="metadataupdate"/>
        <assetupdate
            jcr:primaryType="nt:unstructured"
            text="Asset Update"
            value="assetupdate"/>
        <assetmetadataupdate
            jcr:primaryType="nt:unstructured"
            text="Asset and Metdata update"
            value="assetmetadataupdate"/>
