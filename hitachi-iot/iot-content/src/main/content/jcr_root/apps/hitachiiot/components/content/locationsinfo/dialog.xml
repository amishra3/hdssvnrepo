<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks"
                        fieldLabel="Location Details"
                        name="./multilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <locationtitle
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Location Title"
                                name="./locationtitle"
                                xtype="textfield"/>
                            <locationaddress
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Location Address"
                                height="125"
                                name="./locationaddress"
                                xtype="richtext">
                                <rtePlugins
                                    jcr:primaryType="nt:unstructured"
                                    path="/apps/hdscorp/components/content/richtext/tab_editor/items/editor/rtePlugins.infinity.json"
                                    xtype="cqinclude"/>
                            </locationaddress>
                            <phone
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Phone Number"
                                name="./phone"
                                xtype="textfield"/>
                        </fieldConfig>
                    </multilinks>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Component Title"
                        name="./title"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
