<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <titleiconurl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Icon URL"
                        name="./titleiconurl"
                        xtype="ddpathfield"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Component Title"
                        name="./title"
                        xtype="textfield"/>
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks"
                        fieldLabel="Location Details"
                        name="./multilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <partnericonimgpath
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Partner Icon Image Path"
                                name="./partnericonimgpath"
                                xtype="ddpathfield"/>
                            <partnericonurl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Partner Icon URL"
                                name="./partnericonurl"
                                xtype="ddpathfield"/>
                            <partnericonalttext
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Partner Icon Image Alt Text"
                                name="./partnericonalttext"
                                xtype="textfield"/>
                            <icontargettype
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window?"
                                name="./icontargettype"
                                type="checkbox"
                                xtype="selection"/>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
