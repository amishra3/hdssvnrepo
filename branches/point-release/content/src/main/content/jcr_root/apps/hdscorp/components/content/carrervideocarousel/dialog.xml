<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="600"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks"
                        fieldLabel="Carrer Page Video Carsoul"
                        name="./multilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <carrerpagebartitle
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Carrer Bar Title"
                                name="./carrerpagebartitle"
                                xtype="textfield"/>
                            <carrertitle
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Banner Title"
                                name="./carrertitle"
                                xtype="textfield"/>
                            <carrerbannerdescription
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Bannner Description"
                                name="./carrerbannerdescription"
                                xtype="textarea"/>
                            <carrerbannerimagepath
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Image Path"
                                name="./carrerbannerimagepath"
                                xtype="pathfield"/>
                            <mobileimagePath
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Banner Mobile Image (Optional)"
                                name="./mobileimage"
                                xtype="ddpathfield"/>
                            <carrervideoiconpath
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Video Icon Path"
                                name="./carrervideoiconpath"
                                xtype="pathfield"/>
                            <carrerbutton
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Button Label"
                                name="./carrerbutton"
                                xtype="textfield"/>
                            <carrervideoembededcode
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Video ID / URL"
                                name="./carrervideoembededcode"
                                xtype="textarea"/>
                            <openoverlay
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open video overlay ?"
                                name="./voverlay"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </openoverlay>
                            <newwin
                                jcr:primaryType="cq:Widget"
                                fieldLabel="open in new window?"
                                name="./newwin"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </newwin>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
