<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items
                    jcr:primaryType="cq:WidgetCollection"
                    xtype="multifieldpanel">
                    <multilinks
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./multilinks"
                        fieldLabel="Partner Carsoul"
                        name="./multilinks"
                        xtype="custommultifield">
                        <fieldConfig jcr:primaryType="cq:WidgetCollection">
                            <partnerpagebartitle
                                jcr:primaryType="cq:Widget"
                                allowBlank="false"
                                fieldLabel="Partner Bar Title"
                                name="./partnerpagebartitle"
                                xtype="textfield"/>
                            <bannertitle
                                jcr:primaryType="cq:Widget"
                                allowBlank="false"
                                fieldLabel="Banner Title"
                                name="./bannertitle"
                                xtype="textfield"/>
                            <bannerdescription
                                jcr:primaryType="cq:Widget"
                                allowBlank="false"
                                fieldLabel="Bannner Description"
                                name="./bannerdescription"
                                xtype="textarea"/>
                            <smallimagepath
                                jcr:primaryType="cq:Widget"
                                fieldLabel="PartnerLogo"
                                name="./imagepath"
                                xtype="pathfield"/>
                            <bannerimagepath
                                jcr:primaryType="cq:Widget"
                                allowBlank="{Boolean}false"
                                fieldLabel="Banner Image Path"
                                name="./bannerimagepath"
                                xtype="pathfield"/>
                            <mobileimagePath
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Banner Mobile Image (Optional)"
                                name="./mobileimage"
                                xtype="ddpathfield"/>
                            <ctalabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CTA Label"
                                name="./ctalabel"
                                xtype="textfield"/>
                            <ctatargeturl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Target URL/ Video ID"
                                name="./ctatargeturl"
                                xtype="pathfield"/>
                            <openinnewwindow
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open in New Window ?"
                                name="./openinnewwindow"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="true"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="false"/>
                                </options>
                            </openinnewwindow>
                            <thirdparty
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Show third party icon?"
                                name="./thirdparty"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </thirdparty>
                            <openoverlay
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open video overlay ?"
                                name="./voverlay"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <Yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="1"/>
                                    <No
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="2"/>
                                </options>
                            </openoverlay>
                        </fieldConfig>
                    </multilinks>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
